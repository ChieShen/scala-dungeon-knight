<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" styleClass="background" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ultra.dungeonknight.view.EnterNameDialogController">
   <children>
      <GridPane layoutX="170.0" layoutY="166.0" prefWidth="300.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="162.0" minWidth="10.0" prefWidth="87.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="213.0" minWidth="10.0" prefWidth="213.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Name:" />
            <TextField fx:id="nameTextField" prefHeight="31.0" prefWidth="125.0" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Label layoutX="238.0" layoutY="68.0" text="Dungeon Knight" />
      <ButtonBar layoutX="170.0" layoutY="329.0" prefHeight="40.0" prefWidth="300.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#backClicked" prefWidth="150.0" text="Back" />
            <Button defaultButton="true" mnemonicParsing="false" onAction="#startSelected" text="Start" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
