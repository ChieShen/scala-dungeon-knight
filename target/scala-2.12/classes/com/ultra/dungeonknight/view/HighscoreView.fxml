<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" styleClass="background" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ultra.dungeonknight.view.HighscoreViewController">
   <children>
      <Label alignment="TOP_CENTER" layoutX="607.0" layoutY="32.0" text="Highscore" />
      <TableView fx:id="playerTable" layoutX="483.0" layoutY="172.0" prefHeight="400.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="100.0">
        <columns>
          <TableColumn fx:id="playerNameColumn" prefWidth="216.0" text="Player Name" />
            <TableColumn fx:id="playerLevelColumn" minWidth="216.0" prefWidth="75.0" text="Player Level" />
          <TableColumn fx:id="waveReachedColumn" minWidth="0.0" prefWidth="216.0" text="Wave reached" />
            <TableColumn fx:id="weaponNameColumn" prefWidth="216.0" text="Last Weapon Used" />
            <TableColumn fx:id="highestDamageColumn" minWidth="0.0" prefWidth="216.0" text="Highest Damage" />
        </columns>
      </TableView>
      <Button layoutX="527.0" layoutY="582.0" mnemonicParsing="false" onAction="#mainMenuClicked" text="Back To Main Menu" />
   </children>
</AnchorPane>
